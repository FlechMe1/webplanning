jQuery('#event .modal-content').html('<%= j render "events/form", event: @event %>');
jQuery('#event').modal('toggle');

// INIT COCOON DELETE FIELD
jQuery('a.remove_fields').on('click', function(e) {
  jQuery(this).parents('.form-inline').fadeOut();
});


jQuery('.is_public_team').on('click', function(){
  if (jQuery(this).is(':checked')) {
    jQuery('#assigns').hide();
  }else{
    jQuery('#assigns').show();
  }
});